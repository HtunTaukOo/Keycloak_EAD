{
    "realm": "enterprise-realm",
    "enabled": true,
    "roles": {
        "realm": [
            {
                "name": "user"
            },
            {
                "name": "admin"
            },
            {
                "name": "moderator"
            }
        ]
    },
    "clients": [
        {
            "clientId": "node-app",
